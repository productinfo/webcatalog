{
  "name": "Juli",
  "description": "AppView template for WebCatalog",
  "version": "8.0.1",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "npx patch-package && npx electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src --ext js",
    "lint:fix": "eslint ./public ./src --ext js --fix",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "yarn build && node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/atomery/webcatalog",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "dependencies": {
    "@cliqz/adblocker-electron": "1.10.0",
    "cheerio": "1.0.0-rc.3",
    "electron": "8.2.0",
    "electron-context-menu": "0.16.0",
    "electron-is-dev": "1.1.0",
    "electron-settings": "3.2.0",
    "electron-window-state": "5.0.3",
    "follow-redirects": "1.10.0",
    "fs-extra": "9.0.0",
    "is-url": "1.2.4",
    "jimp": "0.9.6",
    "menubar": "8.0.1",
    "node-fetch": "2.6.0",
    "proxy-agent": "3.1.1",
    "semver": "7.1.3",
    "tmp": "0.1.0",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.9.7",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "concurrently": "5.1.0",
    "cross-env": "7.0.2",
    "date-fns": "2.11.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "lodash.camelcase": "4.3.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.1",
    "react-sortable-hoc": "1.11.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.75",
    "wait-on": "4.0.1"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
