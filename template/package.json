{
  "name": "Juli",
  "description": "AppView template for WebCatalog",
  "version": "5.0.1",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "npx patch-package && npx electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src --ext js",
    "lint:fix": "eslint ./public ./src --ext js --fix",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "yarn build && node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/quanglam2807/webcatalog",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "dependencies": {
    "electron": "7.0.0",
    "electron-is-dev": "1.1.0",
    "electron-settings": "3.2.0",
    "electron-spellchecker": "2.2.0",
    "electron-window-state": "5.0.3",
    "follow-redirects": "1.9.0",
    "fs-extra": "8.1.0",
    "jimp": "0.6.8",
    "menubar": "7.0.0",
    "node-fetch": "2.6.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "semver": "6.3.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "concurrently": "5.0.0",
    "cross-env": "6.0.3",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "lodash.camelcase": "4.3.0",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "6.0.1",
    "react-scripts": "3.2.0",
    "react-sortable-hoc": "1.10.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.75",
    "wait-on": "3.3.0"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
